<mvc:View controllerName="html_mod.html_mod.controller.Splash" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout"
	xmlns:u="sap.ui.unified" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app" class="sapUiResponsiveMargin">
			<pages>
				<Page id="page" title="{i18n>title}" showHeader="false">
					<content>
						
						
						<ObjectHeader title="IBP Test Automation" condensed="true">
							
						</ObjectHeader>
						
						
						<GenericTile class="sapUiTinyMarginBegin sapUiMediumMarginTop tileLayout" header="Enter System Info" press="sysInfoSelect">
							<TileContent footer="Step 1">
								<ImageContent id="tile1Icon" src="sap-icon://laptop"/>
							</TileContent>
						</GenericTile>
						<GenericTile id="tile2" blocked="true" class="sapUiTinyMarginBegin sapUiMediumMarginTop tileLayout" header="Data Submission"
							press="homeSelect">
							<TileContent footer="Step 2">
								<ImageContent id="tile2Icon" src="sap-icon://home-share"/>
							</TileContent>
						</GenericTile>
						<GenericTile id="tile3" blocked="true" class="sapUiTinyMarginBegin sapUiMediumMarginTop tileLayout" header="Scenario Data Analysis"
							press="dataAnalysisSelect">
							<TileContent footer="Analyze Data">
								<ImageContent src="sap-icon://business-objects-experience"/>
							</TileContent>
						</GenericTile>
						<GenericTile id="tile4" blocked="true" class="sapUiTinyMarginBegin sapUiMediumMarginTop tileLayout" header="Manage Data" press="manageDataSelect">
							<TileContent footer="View/Delete Your Data">
								<ImageContent src="sap-icon://crm-service-manager"/>
							</TileContent>
						</GenericTile>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>